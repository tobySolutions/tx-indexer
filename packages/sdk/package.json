{
  "name": "tx-indexer",
  "version": "0.5.1",
  "description": "A TypeScript SDK that transforms raw Solana transactions into human-readable financial data with automatic classification and protocol detection",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "bun run build",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "bun test",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "keywords": [
    "solana",
    "blockchain",
    "transaction",
    "indexer",
    "classifier",
    "sdk",
    "web3",
    "defi",
    "typescript",
    "transaction-classification"
  ],
  "author": "cxalem",
  "repository": {
    "type": "git",
    "url": "https://github.com/cxalem/tx-indexer.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/cxalem/tx-indexer#readme",
  "bugs": "https://github.com/cxalem/tx-indexer/issues",
  "license": "MIT",
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@size-limit/esbuild": "^12.0.0",
    "@size-limit/esbuild-why": "^12.0.0",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@types/bun": "latest",
    "eslint": "^9.39.1",
    "size-limit": "^12.0.0",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5",
    "@solana/kit": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.1.13"
  }
}
